@model IEnumerable<String>

@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Kinks Roleplay</title>
</head>
<body>
    <link rel="stylesheet" type="text/css" href="~/css/main.css" />
    
    <div id="container">
        <video autoplay muted loop>
            <source src="~/videos/light.mp4" type="audio/mp4">
        </video>

        <section id="main_content">
            <div id="head">
                <h1>Kinks RP</h1>
                <p class="sub_head">Friendliest community ever</p>
            </div>

            <div id="links">
               <a href="http://wow.kinksrp.com/"> <img   class="subdomainlogo" src="~/images/wow.png" /></a>
            </div>
        </section>

        <div class="footer">
            <div class="footer-text">
                <a class="footer-text" href="https://github.com/Antonystar/kinksrp">
                    KinksRP
                </a>
                © 2017
            </div>
        </div>
    </div>
</body>
</html>
